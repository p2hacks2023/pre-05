.japaneseFont {
    -unity-font-definition: url('/Assets/Fungus/Fonts/Amaranth/Amaranth-Regular.otf');
    font-size: 30px;
}
.left {
    position: absolute;
    left: 5%;
    top: 0px;
}
.frameBlack {
    background-color: black;
    border-radius: 0% 0% 5px 5px; 
}
.frameBlue {
    background-color: #AAC8FF;
    border-radius: 0% 0% 5px 5px; 
    margin-left: 3px;
    margin-right: 3px;
    margin-bottom: 3px;
    flex-direction: row;
}
.time {
    flex-direction: row;
    padding-right: 10px;
    padding-left: 10px;
    color: white;
}
.hour {
    margin-right: 0px;
}
.minute {
    margin-left: 0px;
}
.templatureFrame {
    flex-direction: row;
    padding-right: 10px;
}
.templatureFrame Label {
    padding-right: 0px;
    padding-left: 0px;
    margin-right: 0px;
    margin-left: 0px;
    color: white;
}
.right {
    position: absolute;
    top: -10%;
    right: 100px;
    height: auto;
    width: 200px;
}
.meterFrame {
    background-image: url('/Assets/UI/Images/meter.png');
    height: 700px;
    width: 400px;
}
.meterBackground {
    position: relative;
    top: 180px;
    left: 164px;
    background-color: #AAC8FF;
    height: 360px;
    width: 70px;
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
}
.meterFrontground {
    background-color: #ffffff;
    height: 100px;
    width: 100%;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
}
.popup {
    position: absolute;
    align-self: center;
    top: 20%;
    height: 600px;
    width: 700px;
}
.goOutside {
    background-image: url('/Assets/UI/Images/goOutside2.PNG');
    height: 100%;
    width: auto;
    flex-direction: row;

}
.Yes {
    background-image: url('/Assets/UI/Images/Yes.PNG');
    height: 10%;
    width: 10%;
    top: 50%;
    left: 30%;
}
.No {
    background-image: url('/Assets/UI/Images/No.PNG');
    height: 10%;
    width: 10%;
    top: 50%;
    left: 50%;
}